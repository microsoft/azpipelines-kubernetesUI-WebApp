    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "file-loader": "~2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-inspect": "^1.11.5",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "rimraf": "~2.6.2",
    "sass": "^1.15.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tar": "^4.4.15",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.1.2"
  }
